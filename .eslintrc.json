{
  "extends": "airbnb-base",
  "plugins": [
      "import"
  ],
  "rules": {
    "no-use-before-define": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "consistent-return": "off",
    "no-param-reassign": "off",
    "no-shadow": ["error", { "allow": ["err"] }],
    "func-names": "off"
  }
}