
<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8">
      {% for type, messages in messages %}
      {% for message in messages %}
      <div class="alert alert-{{ type }}" role="alert">
        {{ message }}
      </div>
      {% endfor %}
      {% endfor %}
    </div>
  </div>
</div>

